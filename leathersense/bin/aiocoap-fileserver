import re
import sys
from aiocoap.cli.fileserver import FileServerProgram
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(FileServerProgram.sync_main())
